{
  "presets": ["react-app"],
  "plugins": [
    [
      "react-css-modules",
      {
        "generateScopedName": "[name]__[local]___[hash:base64:5]",
        "filetypes": {
          ".scss": {
            "syntax": "postcss-scss"
          }
        }
      }
    ],
    ["import", { "libraryName": "antd-mobile", "style": "css" }], // `style: true` 会加载 less 文件
  ]
}